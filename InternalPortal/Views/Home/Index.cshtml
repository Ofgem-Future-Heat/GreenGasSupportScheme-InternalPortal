@model InternalPortal.ViewModels.Dashboard

@{
    ViewData["Title"] = "Home Page";
}

<div class="govuk-grid-row">
    <div class="govuk-grid-column-two-full">
        <h1 class="govuk-heading-xl" style="margin-bottom: 20px;">
            Dashboard
        </h1>

        @{ await Html.RenderPartialAsync("_Organisations", Model.Organisations); }

        @{ await Html.RenderPartialAsync("_Applications", Model.Applications.StageOneSubmitted, new ViewDataDictionary(ViewData) { { "Heading", "Stage 1 submitted" } }); }

        @{ await Html.RenderPartialAsync("_Applications", Model.Applications.StageTwoSubmitted, new ViewDataDictionary(ViewData) { { "Heading", "Stage 2 submitted" } }); }

    </div>
</div>